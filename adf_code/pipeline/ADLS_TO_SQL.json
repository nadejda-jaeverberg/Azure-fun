{
	"name": "ADLS_TO_SQL",
	"properties": {
		"activities": [
			{
				"name": "Read_config",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "DS_Json",
						"type": "DatasetReference",
						"parameters": {
							"container_name": {
								"value": "@pipeline().globalParameters.datacontainer",
								"type": "Expression"
							},
							"folder_name": "configs",
							"file_name": "adls_to_sql.json"
						}
					}
				}
			}
		],
		"annotations": []
	}
}